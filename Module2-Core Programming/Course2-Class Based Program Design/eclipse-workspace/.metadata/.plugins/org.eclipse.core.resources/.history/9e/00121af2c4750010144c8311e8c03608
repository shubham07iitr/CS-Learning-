//INSERTSEARCH ALGO

interface IListNew<T> {
	IListNew<T> sort(); //sorts the given list as per the comparator
	IListNew<T> insert(T element); // adds a given element in a sorted list of elements
}

class MtListNew<T> implements IListNew<T> {
	//Defining the constructor
	MtListNew() {}
	
	public IListNew<T> sort() {return this;} //sorting an empty list returns an empty list
	public IListNew<T> insert(T element) {return new ConsListNew<T>(element, this);}
}

class ConsListNew<T> implements IListNew<T> {
	T first;
	IList<T> rest;
	
	//Defining the constructor
	ConsListNew(T first, IList<T> rest) {this.first = first; this.rest = rest;}
	
	//METHODS
	public IListNew<T> sort() {
		return this.rest.sort().insert(this.first);} //inserts the given first element in a sorted list oof rest of elements
	
