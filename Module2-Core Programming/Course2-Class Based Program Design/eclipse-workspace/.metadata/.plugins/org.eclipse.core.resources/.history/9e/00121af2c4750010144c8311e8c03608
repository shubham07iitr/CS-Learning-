import tester.*;
import java.util.*;

//ICOMPARE Interface

interface ICompare<T> {
	int compare(T e1, T e2);}

class CompareInts implements ICompare<Integer> {
	public int compare (Integer e1, Integer e2) {return e1-e2;}
}



